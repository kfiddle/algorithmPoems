ight: 900;\r\n  font-size: 2.1rem;\r\n  position: absolute;\r\n  top: 60%;\r\n  left: 5%;\r\n}\r\n\r\n.projectModal {\r\n  position: absolute;\r\n  flex-direction: column;\r\n  background: white;\r\n  background: linear-gradient(to Left, black, #5a3939);\r\n  width: 80vw;\r\n  height: 100vh;\r\n  transform: translateX(100vw);\r\n  top: 10vh;\r\n  right: 0;\r\n  transition: 0.75s cubic-bezier(0.4, 0, 0.4, 1);\r\n}\r\n\r\n.projectModal.opened {\r\n  background: linear-gradient(to Left, black, black, black,#43415a);\r\n  transition: 1.75s cubic-bezier(0.4, 0, 0.4, 1);\r\n}\r\n\r\n.xOut {\r\n  padding: 1rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.innerModalDiv {\r\n  display: grid;\r\n  grid-template-columns: 2fr 3fr;\r\n  margin-left: 5%;\r\n  height: 75%;\r\n  width: 90%;\r\n}\r\n\r\n.descriptionDiv {\r\n  background-color: transparent;\r\n}\r\n\r\n.descriptionText {\r\n  color:white;\r\n  font-size: 1.5rem;\r\n  line-height: 2em;\r\n  letter-spacing: 0.15em;\r\n  margin: 1rem;\r\n}\r\n\r\n.slideDiv {\r\n\r\n  position: relative;\r\n}\r\n\r\n.slideImageDiv{\r\n  margin: 0 auto;\r\n  width: 90%;\r\n  height: 90%;\r\n  background-size: contain;\r\n  background-repeat: no-repeat;\r\n}\r\n\r\n.carouselButtons {\r\n  position: absolute;\r\n  top: 75%;\r\n  cursor: pointer;\r\n}\r\n\r\n.arrow-left {\r\n  left: 17%;\r\n  background-color: transparent;\r\n  border-bottom: 3rem solid transparent;\r\n  border-top: 3rem solid transparent;\r\n  border-right: 1.5rem solid white;\r\n  border-left: none;\r\n}\r\n\r\n.arrow-right {\r\n  right: 13%;\r\n  background-color: transparent;\r\n  border-bottom: 3rem solid transparent;\r\n  border-top: 3rem solid transparent;\r\n  border-left: 1.5rem solid white;\r\n  border-right: none;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"41e7d0e0fdd72cea79eeb8f2042baa19","cacheData":{"env":{}}}